# OpenClaw GCP Deployment — Configuration Template
# Copy to terraform/terraform.tfvars and fill in values.
# Never commit terraform.tfvars to version control (it's in .gitignore).

# ── Required ──────────────────────────────────

# GCP project ID (find in GCP Console → Project Selector)
project_id = "your-gcp-project-id"

# Shared token that Claude Code clients must present to connect to the gateway.
# Choose any strong random string (e.g. `openssl rand -hex 32`).
gateway_token = "your-strong-random-token-here"

# ── LLM API Keys (at least one required) ──────

# Groq — free, fast, Llama 3.3 70B — recommended starting point
# Sign up: https://console.groq.com
groq_api_key = "gsk_..."

# Google Gemini — free tier (15 RPM, 1M tokens/day)
# Get key: https://aistudio.google.com/app/apikey
gemini_api_key = "AIza..."

# Anthropic Claude — pay-per-use, no free tier
# Sign up: https://console.anthropic.com
anthropic_api_key = ""

# ── Optional ──────────────────────────────────

# GCP region (default: us-central1)
# region = "us-central1"

# Container image (default: docker.io/alpine/openclaw:latest)
# container_image = "docker.io/alpine/openclaw:latest"
